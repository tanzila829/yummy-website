<main id="main" class="main">

    <div class="pagetitle">
        <h1>Dashboard</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                <li class="breadcrumb-item active">Create</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
    <section class="section dashboard">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Floating labels Form</h5>
    <!-- Floating Labels Form -->
<form class="raw g-3" method="POST" action="/admin/team/store" >
<div class="col-md-6">
                  <div class="form-floating">
                    <input type="text" class="form-control form-control-sm" id="floatingName" placeholder="Name" name="Name">
                    <label for="floatingName">Name</label>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="form-floating">
                    <input type="text" class="form-control form-control-sm" id="floatingName" placeholder="designation" name="designation">
                    <label for="floatingName">Designation</label>
                  </div>
                </div>


                <div class="col-12">
                  <div class="form-floating">
                    <textarea class="form-control form-control-sm" name="facebook" placeholder="facebook" id="floatingTextarea" style="height: 100px;" ></textarea>
                    <label for="floatingTextarea">facebook</label>
                  </div>
                </div>
                <div class="col-12">
                  <div class="form-floating">
                    <textarea class="form-control form-control-sm" name="instragram" placeholder="instragram" id="floatingTextarea" style="height: 100px;" ></textarea>
                    <label for="floatingTextarea">instragram</label>
                  </div>
                </div>
                <div class="col-12">
                  <div class="form-floating">
                    <textarea class="form-control form-control-sm" name="twitter" placeholder="twitter" id="floatingTextarea" style="height: 100px;" ></textarea>
                    <label for="floatingTextarea">twitter</label>
                  </div>
                </div>
                <div class="col-12">
                  <div class="form-floating">
                    <textarea class="form-control form-control-sm" name="linkedin" placeholder="linkedin" id="floatingTextarea" style="height: 100px;" ></textarea>
                    <label for="floatingTextarea">linkedin</label>
                  </div>
                </div>
                <div class="col-md-12">
                  <div class="col-md-12">
                        <label for="floatingCity">Image</label>
                    <div class="form-floating">
                      <input type="file" class="form-control form-control-sm" name="image" id="floatingCity">
                      
                    </div>
                  </div>
                </div>
                <div class="text-center">
                  <button type="submit" class="btn btn-primary">Submit</button>
                  <button type="reset" class="btn btn-secondary">Reset</button>
                </div>
              </form><!-- End floating Labels Form -->

            </div>
          </div>
    </section>
</main>
