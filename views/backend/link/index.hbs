{{!-- <main id="main" class="main"> --}}

    <div class="pagetitle">
        <h1>Dashboard</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/admin">Home</a></li>
                <li class="breadcrumb-item active">{{title}}</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
{{!-- class="section dashboard" --}}
    <section >

          <div class="card">
            <div class="card-body">
                <a class="btn btn-outline-warning btn-sm float-end my-2" href="/admin/link/create">Create</a>

              <!-- Dark Table -->
              <table class="table table-dark">
                <thead>
                  <tr>
                   
                     <th scope="col">facebook</th>
                      <th scope="col">twiter</th>
                       <th scope="col">instragram</th>
                            <th scope="col">linkedin</th>
                    <th scope="col">Action</th>
                  </tr>
                </thead>
              <tbody>
                 {{#each link}}
                  <tr>
                    
                    <td>{{facebook}}</td>
                    <td>{{twiter}}</td>
                    <td>{{instragram}}</td>
                    <td>{{linkedin}}</td>
                    
                    <td>
                        <a href="/admin/link/{{id}}/show" class="btn btn-outline-info btn-sm">Show</a>
                        <a href="/admin/link/{{id}}/edit" class="btn btn-outline-primary btn-sm">Edit</a>
                        <a href="/admin/link/{{id}}/delete" class="btn btn-outline-danger btn-sm">Delete</a>
                    </td>
                  </tr>
                   
                  {{/each}}
                </tbody>
              </table>
              <!-- End Dark Table -->

            </div>
          </div>
    </section>
{{!-- </main> --}}